{
    "compilerOptions": {
        "jsx": "react",
        "sourceMap": true,
        "module": "commonjs",
        "declaration": true,
        "skipLibCheck": true,
        "target": "esnext",
        "typeRoots": ["../../node_modules/@types"],
        "types": ["node", "jest"],
        "esModuleInterop": true,
        "outDir": "./build/",
        "emitDeclarationOnly": true
    },
    "include": ["./index.ts", "src", "@types"],
    "exclude": ["build"]
}
