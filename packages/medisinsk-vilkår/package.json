        "@types/jest": "29.5.2",
        "@types/node": "18.16.18",
        "@types/react": "18.2.13",
        "@types/react-dom": "18.2.6",
        "@types/react-modal": "3.16.0",
        "@types/testing-library__jest-dom": "5.14.6"
    },
    "dependencies": {
        "@navikt/ds-css": "4.3.0",
