        "@navikt/k9-fe-date-utils": "^1.0.2",
        "@navikt/k9-fe-form-utils": "^1.0.6",
        "@navikt/k9-fe-http-utils": "^1.0.4",
        "@navikt/k9-fe-period-utils": "^1.0.5",
